{
  "name": "eslint-config-ccrazy88-react",
  "version": "1.3.0",
  "description": "A personalized, shareable ESLint configuration.",
  "license": "MIT",
  "author": "Chrisna Aing <chrisna@chrisna.org>",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/crazy88/eslint-config-ccrazy88-react.git"
  },
  "scripts": {
    "precommit": "npm run test && lint-staged",
    "test": "npm run conflicting-rules && npm run unused-rules",
    "conflicting-rules": "eslint --print-config .eslintrc | eslint-config-prettier-check",
    "unused-rules": "eslint-find-rules --unused ./index.js"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.json": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "eslint-config-ccrazy88": "^1.3.0"
  },
  "devDependencies": {
    "eslint": "^5.3.0",
    "eslint-find-rules": "^3.3.1",
    "eslint-plugin-html": "^4.0.5",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-prettier": "^2.6.2",
    "eslint-plugin-react": "^7.10.0",
    "husky": "^0.14.3",
    "lint-staged": "^7.2.0",
    "prettier": "^1.14.0"
  },
  "peerDependencies": {
    "eslint": "^5.3.0",
    "eslint-plugin-html": "^4.0.5",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-prettier": "^2.6.2",
    "eslint-plugin-react": "^7.10.0"
  }
}
